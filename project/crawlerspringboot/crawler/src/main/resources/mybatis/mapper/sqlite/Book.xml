<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ifurion.crawlerspringboot.dao.BookDao">
    <insert id="insert">
        INSERT INTO book
        (title,comments)
        VALUES
        (#{title},#{comments});
    </insert>
    <select id="selectAll" resultType="com.ifurion.crawlerspringboot.entity.Book">
        SELECT *
        FROM book
    </select>
</mapper>
